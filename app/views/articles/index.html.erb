  <h1>The Hub News</h1>
  <div class="articles">
    <% @articles.each do |article| %>
      <div class="article_box w-1/3" >
        <h3><%= article.title %></h3>
        <p><%= truncate(article.content, :length => 100, :separator => ' ', :ommision => "... (Read More)") %></p>
      </div>
    <% end %>
  </div>